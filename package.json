{
  "name": "nuxt7",
  "version": "0.0.7",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "dev": "nuxt dev -c kitchen-sink/nuxt.config.js",
    "dev-build": "nuxt build -c kitchen-sink/nuxt.config.js",
    "dev-analyze": "nuxt build -c kitchen-sink/nuxt.config.js --analyze",
    "dev-start": "nuxt start --spa -c kitchen-sink/nuxt.config.js",
    "dev-generate": "nuxt generate -c kitchen-sink/nuxt.config.js",
    "lint": "eslint --ext .js,.vue .",
    "test": "npm run lint",
    "precommit": "lint-staged",
    "surge": "surge -d https://nuxt7.surge.sh demo-dist"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "@nuxtjs/icon": "^1.1.0",
    "@nuxtjs/manifest": "^1.6.1",
    "@nuxtjs/meta": "^1.5.0",
    "@nuxtjs/pwa": "^0.2.1",
    "@pi0/framework7": "^2.0.0-beta.8.4",
    "@pi0/framework7-vue": "^2.0.0-beta.1.3",
    "framework7-icons": "^0.8.9",
    "nuxt": "^1.0.0-rc11"
  },
  "files": [
    "lib",
    "assets",
    "templates"
  ],
  "devDependencies": {
    "eslint": "^4.7.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.2",
    "surge": "^0.19.0"
  }
}
