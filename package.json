{
  "name": "nuxt7",
  "version": "4.0.2",
  "license": "MIT",
  "main": "dist/module.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "siroc && mkdist --src src/runtime --dist dist/runtime",
    "dev": "yarn ks:dev",
    "ks:analyze": "nuxt build --analyze examples/kitchen-sink",
    "ks:build": "nuxt build examples/kitchen-sink",
    "ks:dev": "nuxt dev examples/kitchen-sink",
    "ks:generate": "nuxt generate examples/kitchen-sink",
    "ks:start": "nuxt start --spa examples/kitchen-sink",
    "lint": "eslint --ext .js,.vue lib",
    "release": "standard-version && git push --follow-tags && npm publish",
    "test": "yarn lint"
  },
  "dependencies": {
    "defu": "3.2.2",
    "framework7": "^5.7.14",
    "framework7-icons": "^3.0.1",
    "framework7-vue": "^5.7.14",
    "less": "^4.1.0",
    "less-loader": "^6",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.12",
    "@nuxtjs/eslint-config": "latest",
    "@nuxtjs/pwa": "latest",
    "eslint": "latest",
    "mkdist": "^0.1.0",
    "nuxt-edge": "latest",
    "siroc": "^0.6.2",
    "standard-version": "latest",
    "surge": "latest"
  }
}
