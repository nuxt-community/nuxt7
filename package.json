{
  "name": "nuxt7",
  "version": "2.0.4",
  "license": "MIT",
  "main": "lib/module.js",
  "scripts": {
    "dev": "npm run ks",
    "ks": "nuxt dev examples/kitchen-sink",
    "ks-build": "nuxt build examples/kitchen-sink",
    "ks-analyze": "nuxt build --analyze examples/kitchen-sink",
    "ks-start": "nuxt start --spa examples/kitchen-sink",
    "ks-generate": "nuxt generate examples/kitchen-sink",
    "lint": "eslint --ext .js,.vue .",
    "test": "npm run lint",
    "precommit": "lint-staged",
    "surge": "surge -d https://nuxt7.cf examples/kitchen-sink/dist",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.6.0",
    "framework7": "^2.3.1",
    "framework7-vue": "^2.3.0",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.3.0",
    "nuxt-edge": "^2.0.0-25638738.d69b4b8",
    "standard-version": "^4.4.0",
    "surge": "^0.20.1"
  }
}
