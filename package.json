{
  "name": "nuxt7",
  "version": "3.0.0",
  "license": "MIT",
  "main": "lib/module.js",
  "scripts": {
    "dev": "npm run ks",
    "ks": "nuxt dev examples/kitchen-sink",
    "ks-build": "nuxt build examples/kitchen-sink",
    "ks-analyze": "nuxt build --analyze examples/kitchen-sink",
    "ks-start": "nuxt start --spa examples/kitchen-sink",
    "ks-generate": "nuxt generate examples/kitchen-sink",
    "lint": "eslint --ext .js,.vue lib",
    "test": "npm run lint",
    "precommit": "lint-staged",
    "surge": "surge -d https://nuxt7.surge.sh examples/kitchen-sink/dist",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "defu": "^0.0.1",
    "framework7": "^4.1.1",
    "framework7-vue": "^4.1.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0"
  },
  "devDependencies": {
    "@nuxtjs/pwa": "^3.0.0-beta.14",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "nuxt-edge": "^2.5.0-25874561.ff93d14b",
    "standard-version": "^5.0.2",
    "surge": "^0.20.3"
  }
}
